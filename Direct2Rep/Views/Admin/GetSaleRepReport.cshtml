                <div class="row">
                    <form action="@Url.Action("GetSaleRepReport", "Admin")" method="post">
                        <div class="col-md-offset-2 col-md-3">
                            <label class="label-absolute">by Sales Reprsentative</label>
                            <select class="form-control" name="Salerep_Id">
                                <option value=@ViewBag.salerep.Id>@ViewBag.salerep.Name</option>
                                <option value="">Select one</option>
                                @if (ViewBag.salesrepscount > 0)
                                {
                                    foreach (SaleRepresentative c in ViewBag.salesreps)
                                    {
                                        <option value=@c.Id>@c.Name</option>
                                    }
                                }|
                            </select>
                        </div>
                        <div class="col-md-2">
                            <label class="label-absolute">Start Date</label>
                            @if (ViewBag.startdate == null)
                            {
                                <input type="date" class="form-control" name="startdate" required>
                            }
                            else
                            {
                                <input type="text" class="form-control dateField" value="@ViewBag.startdate.ToString("dd-MM-yyyy")" id="Date1" name="startdate" onfocus="DateType('Date')" onblur="" required>
                            }
                        </div>
                        <script>
                            $("input").on("change", function () {
                                this.setAttribute(
                                    "data-date",
                                    moment(this.value, "YYYY-MM-DD")
                                        .format(this.getAttribute("data-date-format"))
                                )
                            }).trigger("change")
                        </script>
                        <div class="col-md-2">
                            <label class="label-absolute">End Date</label>
                            @if (ViewBag.enddate == null)
                            {
                                <input type="date" class="form-control" name="enddate" required>
                            }
                            else
                            {
                                <input type="text" class="form-control dateField" value="@ViewBag.enddate.ToString("dd-MM-yyyy")" id="Date2" name="enddate" onfocus="DateType('Date')" onblur="" required>
                            }
                        </div>
                        <div class="col-md-2">
                            <br />
                            <button type="submit" class="btn btn-primary">Get Report</button>
                        </div>
                    </form>
                </div>
                <br />
                <div class="row">
                    <div class="col-md-12">
                        <!-- BEGIN EXAMPLE TABLE PORTLET-->
                        <div class="portlet light bordered">
                            <div class="portlet-title">
                                <div class="caption font-dark">
                                    <i class=" font-dark"></i>
                                    <span class="caption-subject bold uppercase">Campaign Leads</span>
                                </div>
                            </div>
                            <div class="portlet-body">
                                <table class="table table-striped table-bordered table-hover" id="sample_1">
                                    <thead>
                                        <tr>
                                            <th>Date of Submission</th>
                                            <th>Sales Firm</th>
                                            <th>Sales Rep</th>
                                            <th>Lead Name</th>
                                            <th>Company </th>
                                            <th>Phone</th>
                                            <th>Email</th>
                                            <th>Contact Time</th>
                                            <th>Contact Type</th>
                                            <th>Message</th>
                                            <th>Address</th>
                                            <th>City</th>
                                            <th>State / Province</th>
                                            <th>ZIP /Postal Code</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (ViewBag.visitorscount > 0)
                                        {
                                            foreach (var item in ViewBag.visitors)
                                            {
                                                Campaign c = new AdminBL().getCampaignById(item.Campaign_Id);
                                                var companyname = new AdminBL().getCompanyById(Convert.ToInt32(c.CompanyId)).Name;
                                                Visitor_States vs = new AdminBL().getVisitor_StatesList().Where(x => x.VisitorId == item.Id).FirstOrDefault();


                                                var result = vs.State.Name.Split('(')[0];
                                                <tr>
                                                    <td>@item.Creadted_At.ToString("dd/MM/yyyy")</td>
                                                    <td>@ViewBag.salerep.SalesFirmName</td>
                                                    <td>@ViewBag.salerep.Name</td>
                                                    <td>@item.Name</td>
                                                    <td>@companyname</td>
                                                    <td>@item.Phone</td>
                                                    <td>@item.Email</td>
                                                    <td>@item.BestTime</td>
                                                    <td>@item.BestMethod</td>
                                                    <td>@item.Message</td>
                                                    <td>@item.Address</td>
                                                    <td>@item.City</td>
                                                    <td>@result</td>
                                                    <td>@item.ZipCode</td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!-- END EXAMPLE TABLE PORTLET-->
                    </div>
                </div>
